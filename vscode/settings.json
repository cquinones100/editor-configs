{
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.rulers": [80, 120],
  "editor.tabSize": 2 ,
  "editor.tabCompletion": "on",
  "workbench.editor.enablePreview": false,
  "files.exclude": {
    "**/.git": false,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "editor.suggestSelection": "first",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "eslint.format.enable": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "debug.javascript.autoAttachFilter": "disabled",
  "git.autofetch": true,
  "terminal.integrated.tabs.enabled": true,
  "security.workspace.trust.untrustedFiles": "open",
  "editor.inlineSuggest.enabled": true,
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command prompted": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "Ubuntu (WSL)": {
      "path": "C:\\WINDOWS\\System32\\wsl.exe",
      "args": ["-d", "Ubuntu"]
    }
  },
  "terminal.integrated.defaultProfile.windows": "Ubuntu (WSL)",
  "cSpell.userWords": ["klass", "rubocop", "spacy"],
  "cSpell.diagnosticLevel": "Hint",
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "redhat.telemetry.enabled": false,
  "lldb.library": "/Applications/Xcode.app/Contents/SharedFrameworks/LLDB.framework/Versions/A/LLDB",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "workbench.colorTheme": "Ros√© Pine Moon",
  "github.copilot.advanced": {},
  "swift.sourcekit-lsp.serverArguments": [
    "-Xswiftc",
    "-sdk",
    "-Xswiftc",
    "/Applications/Xcode.app/Contents/Developer/Platforms/iPhoneSimulator.platform/Developer/SDKs/iPhoneSimulator.sdk",
    "-Xswiftc",
    "-target",
    "-Xswiftc",
    "x86_64-apple-ios13.6-simulator"
  ],
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "latex": false
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "http.experimental.systemCertificatesV2": true,
  "breadcrumbs.filePath": "on",
  "breadcrumbs.symbolPath": "off",
  "search.exclude": {
    "**/node_modules": true,
  },
  "workbench.editor.showTabs": "none",
  "workbench.activityBar.location": "hidden",
  "vim.visualModeKeyBindings": [
    {
      "before": ["J"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.moveLinesUpAction"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["<S-y>"],
      "after": ["y", "y"]
    },
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["g", "i"],
      "commands": ["editor.action.referenceSearch.trigger"]
    },
    {
      "before": ["<S-k>"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["g", "c"],
      "commands": ["editor.action.commentLine"]
    },
  ],
  "vim.useSystemClipboard": true,
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
  "workbench.editor.editorActionsLocation": "hidden",
  "editor.formatOnSave": false,
  "task.allowAutomaticTasks": "on",
  "workbench.editorAssociations": {
    "git-rebase-todo": "default"
  },
  "workbench.panel.defaultLocation": "right",
  "grunt.autoDetect": "on",
  "colorize.languages": [
    "javascript",
  ],
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.profiles.osx": {
    "zsh": {
      "path": "/bin/zsh",
      "args": [
        "-l"
      ]
    },
  },
  "multiCommand.commands": [
    {
      "command": "multiCommand.openTerminal",
      "sequence": [
        "workbench.action.terminal.toggleTerminal",
        "workbench.action.positionPanelRight",
        "workbench.action.increaseViewSize",
        "workbench.action.increaseViewSize",
        "workbench.action.increaseViewSize",
        "workbench.action.increaseViewSize",
        "workbench.action.increaseViewSize",
      ]
    }, {
      "command": "multiCommand.stageChanges",
      "sequence": [
        "git.stageSelectedRanges",
        "workbench.action.closeActiveEditor"
      ]
    }, {
      "command": "multiCommand.revertChanges",
      "sequence": [
        "git.revertSelectedRanges",
        "workbench.action.closeActiveEditor"
      ]
    },
    {
      "command": "multiCommand.openCopilotChat",
      "sequence": [
        "workbench.panel.chat.view.copilot.focus",
        "workbench.action.decreaseViewSize",
        "workbench.action.decreaseViewSize",
        "workbench.action.decreaseViewSize",
        "workbench.action.decreaseViewSize",
        "workbench.action.increaseViewSize",
        "workbench.action.increaseViewSize",
        "workbench.action.increaseViewSize",
        "workbench.action.increaseViewSize",
      ]
    }
  ],
  "[ruby]": {
    "editor.formatOnSave": false
  },
  "editor.accessibilitySupport": "off",
  "sync.ignoredSettings": ["*"],
  "sorbet.enabled": true,
  "sorbet.typedFalseCompletionNudges": false,
  "rubyLsp.erbSupport": false
}